{
  "functions": [
    {
      "source": "functions",
      "codebase": "default"
    }
  ],
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "database": {
      "port": 9000
    },
    "pubsub": {
      "port": 8085
    },
    "storage": {
      "port": 9199
    },
    "eventarc": {
      "port": 9299
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "extensions": {
    "storage-extract-image-text": ".",
    "storage-transcode-videos": "../storage-transcode-videos",
    "storage-resize-images-gifs": "firebase/storage-resize-images@0.1.36",
    "storage-resize-images": "firebase/storage-resize-images@0.1.36",
    "firestore-palm-gen-text": "googlecloud/firestore-palm-gen-text@0.1.5"
  }
}
